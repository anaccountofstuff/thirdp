local a="v"local b=game:GetService("Players")local c=b.LocalPlayer;local d=c:GetMouse()local e=false;d.KeyDown:Connect(function(f)if f==a then if e then for g,h in pairs(c.Character:GetDescendants())do if h:IsA("BasePart")or h:IsA("Decal")then h.LocalTransparencyModifier=1 end end;e=false;print("untoggled")else for g,h in pairs(c.Character:GetDescendants())do if h:IsA("BasePart")or h:IsA("Decal")then h.LocalTransparencyModifier=0 end end;e=true;print("toggled")end end end)game:GetService("RunService").RenderStepped:connect(function()if e then game.Workspace.CurrentCamera.CFrame=game.Workspace.CurrentCamera.CFrame*CFrame.new(0,0,10)end end)
