local a=true;local b=game:GetService("Players")local c=b.LocalPlayer;game:GetService("RunService").RenderStepped:connect(function()game.Workspace.CurrentCamera.CFrame=game.Workspace.CurrentCamera.CFrame*CFrame.new(0,0,10)end)while a and wait(.1)do for d,e in pairs(c.Character:GetDescendants())do if e:IsA("BasePart")or e:IsA("Decal")then if e.LocalTransparencyModifier~=0 then e.LocalTransparencyModifier=0 end end end end
